            string _password = (string)Dts.Variables["User::CS_Password"].Value;
            string _dataSource = (string)Dts.Variables["User::CS_DataSource"].Value;
            string _connectionString = (string)(Dts.Connections[_dataSource].ConnectionString);
            return _connectionString.Replace(_provider, _password);
        }

        private DataTable FileList()
        {
            string connectionString = ConnectionString();
            SqlConnection con = new SqlConnection(connectionString);
            SqlCommand cmd = new SqlCommand();
            cmd.CommandType = CommandType.StoredProcedure;
            cmd.Connection = con;
            cmd.CommandText = (string)Dts.Variables["User::SP_FileList"].Value;

            DataTable dt = new DataTable();
            SqlDataAdapter da = new SqlDataAdapter();

            using (con)
            {
                con.Open();
                cmd.ExecuteNonQuery();
                da.SelectCommand = cmd;
                da.Fill(dt);
                con.Close();
            }

            return dt;
        }

