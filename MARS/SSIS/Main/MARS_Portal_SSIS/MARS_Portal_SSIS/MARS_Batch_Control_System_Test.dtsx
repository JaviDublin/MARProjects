        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion



        public void Main()
        {
            //DeleteFiles();
            bool isActive, hasDate;
            string fileMask, filePath, filePathTemp, filePathArchive, batchType , batchTime;

            // Get the files masks and paths from the server. Table [bcs].[FileList]
            foreach (DataRow dr in FileList().Rows)
            {
                isActive = (bool)dr["IsActive"];
                if (isActive)
                {
                    fileMask = dr["FileMask"].ToString();
                    filePath = dr["FilePath"].ToString();
                    filePathTemp = dr["FilePathTemp"].ToString();
                    filePathArchive = dr["FilePathArchive"].ToString();
                    hasDate = (bool)dr["HasDate"];
                    batchType = dr["BatchType"].ToString();
                    batchTime = dr["BatchTime"].ToString();

                    //Move File from Temp to Archive (remove in the Test version)
                    //ArchiveFile(fileMask, filePath, filePathTemp, filePathArchive);

                    // Move File from .root to Temp
                    ProcessFile(fileMask, filePath, filePathTemp, filePathArchive, batchType, hasDate, batchTime);

                    Dts.TaskResult = (int)ScriptResults.Success;
                }
            }
        }

