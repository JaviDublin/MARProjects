        private void ProcessFile(string mask, string path, string pathtemp, string patharchive, string batchtype, bool hasDate, string batchTime)
        {
            string LastDateModified = String.Empty;

            string Action = (string)Dts.Variables["User::Log_Action"].Value;

            string Status = (string)Dts.Variables["User::Log_Status"].Value;

            string Temp = (string)Dts.Variables["User::Folder_Temp"].Value;

            string pattern = @"[0-9][0-9][0-9][0-9]-([0-9]+)-([0-9]+)-" + mask;

            DirectoryInfo di = new DirectoryInfo(path);

            foreach (FileInfo fi in di.GetFiles())
            {
                if (Regex.IsMatch(fi.Name, mask))
                {
                    if (hasDate)
                    {
                        if (File.Exists(path + pathtemp + fi.Name))
                        {
                            File.Delete(path + pathtemp + fi.Name);
                        }

                        //File.Move(path + fi.Name, path + pathtemp + fi.Name);
                        
                        File.Copy(path + fi.Name, path + pathtemp + fi.Name);
                       
                        WriteFileProcessLog(batchtype, Action, Status, fi.Name, "");

                    }
                    else
                    {
                        if (Regex.IsMatch(fi.Name, pattern))
                        {
                            if (File.Exists(path + pathtemp + fi.Name))
                            {
                                File.Delete(path + pathtemp + fi.Name);
                            }

                            File.Copy(path + fi.Name, path + pathtemp + fi.Name);
                            //File.Move(path + fi.Name, path + pathtemp + fi.Name);

                            WriteFileProcessLog(batchtype, Action, Status, fi.Name, "");
                        }
                        else
                        {
                            if (batchTime == "Hourly")
                            {
                                //LastDateModified = fi.LastWriteTime.Year + "-" + fi.LastWriteTime.Month + "-" + fi.LastWriteTime.Day + "-" + fi.LastWriteTime.Hour + "-" + fi.LastWriteTime.Minute + "-";
                                LastDateModified = fi.LastWriteTime.Year + fi.LastWriteTime.Month + fi.LastWriteTime.Day + "_" + fi.LastWriteTime.Hour + fi.LastWriteTime.Minute + fi.LastWriteTime.Second + "_";
                            }
                            else
                            {
                                //LastDateModified = fi.LastWriteTime.Year + "-" + fi.LastWriteTime.Month + "-" + fi.LastWriteTime.Day + "-";
                                LastDateModified = fi.LastWriteTime.Year + fi.LastWriteTime.Month + fi.LastWriteTime.Day + "_"; 
                            }

                            
                            
                            if (File.Exists(path + pathtemp + LastDateModified + fi.Name))
                            {
                                File.Delete(path + pathtemp + LastDateModified + fi.Name);
                            }

                            File.Copy(path + fi.Name, path + pathtemp + LastDateModified + fi.Name);
                            //File.Move(path + fi.Name, path + pathtemp + LastDateModified + fi.Name);

                            WriteFileProcessLog(batchtype, Action, Status, LastDateModified + fi.Name, "");
                        }
                    }
                }
            }
        }

        private void WriteFileProcessLog(string entity, string action, string status, string fileName, string data)
        {
            string connectionString = ConnectionString();
            SqlConnection con = new SqlConnection(connectionString);
            SqlCommand cmd = new SqlCommand();
            cmd.CommandType = CommandType.StoredProcedure;
            cmd.Connection = con;
            cmd.CommandText = (string)Dts.Variables["User::SP_File_Process"].Value;

            cmd.Parameters.AddWithValue("@Entity", entity);
            cmd.Parameters.AddWithValue("@Action", action);
            cmd.Parameters.AddWithValue("@Status", status);
            cmd.Parameters.AddWithValue("@FileName", fileName);
            cmd.Parameters.AddWithValue("@Data", data);

            using (con)
            {
                con.Open();
                cmd.ExecuteNonQuery();
                con.Close();
            }
        }
    }
